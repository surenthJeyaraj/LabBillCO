        <table class="table striped hovered dataTable shadow"  id="tblResult" border="0" aria-describedby="dataTables-1_info"  >
            <thead>
                <tr class="GridHeader"  role="row">
                    <th style="width:12%;white-space:nowrap;font-weight:bold;" >Transaction ID</th>
                    <th style="width:16%;white-space:nowrap;font-weight:bold;" >File Name</th>
                    @*<th style="width:8%;white-space:nowrap;font-weight:bold;" >Posted File</th>*@
                    <th style="width:10%;white-space:nowrap;font-weight:bold;">User ID</th>
                    <th style="width:8%;white-space:nowrap;font-weight:bold;">Date </th>
                    <th style="width:8%;white-space:nowrap;font-weight:bold;" >Version</th>
                    @* <th class="GridHeader">Trans. Type</th>*@
                    <th style="width:4%;white-space:nowrap;font-weight:bold;" >Mode</th>
                    <th style="width:4%;white-space:nowrap;font-weight:bold;" >TA1</th>
                    <th style="width:5%;white-space:nowrap;font-weight:bold;" >997/999</th>
                    <th style="width:12%;white-space:nowrap;font-weight:bold;"></th>
                    <th style="width:8%;white-space:nowrap;font-weight:bold;" ></th>
                    <th style="width:4%;white-space:nowrap;font-weight:bold;" ></th>
                    @* <th class="GridHeader"></th>*@
                    @* <th class="GridHeader">Total</th>*@

                </tr>
            </thead>
            <tbody>
            </tbody>


        </table>
        <div class="dataTables_info" id="dataTables-1_info" role="alert" aria-live="polite" aria-relevant="all"></div>
        <div class="dataTables_paginate paging_simple_numbers" id="dataTables-1_paginate"></div>
        <input type="hidden" value="@ViewBag.PostedDate" id="hidPosteddate" />
        <input type="hidden" value="@ViewBag.CheckStatus" id="hidCheckStatus" />
    </div>

</div>

