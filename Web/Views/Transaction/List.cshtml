@{
    ViewBag.Title = "List Transactions";
    Layout = "~/Views/Shared/AuthenticateLayout.cshtml";
}
<style type="text/css">
    #searchCriteria {
        margin-top: 20px;
    }

        #searchCriteria:before {
            content: "Search";
        }

    #transactionTable {
        margin-top: 20px;
    }

    #transactionTable:before {
        content: "Results";
    }

    /* for window style...*/
    table.gridtable {
        font-family: verdana,arial,sans-serif;
        font-size: 11px;
        color: #333333;
        border-width: 1px;
        border-color: #666666;
        border-collapse: collapse;
    }

    table.gridtable th {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
        background-color: #CEE7C9;
        font-weight: bold;
    }

    table.gridtable td {
        border-width: 1px;
        padding: 8px;
        border-style: solid;
        border-color: #666666;
    }
    .dataTable .notSortable{
    font-weight:bold;
}
 
table.dataTable thead th {
	
	font-weight: bold;
	
	
}




    sStyleParaText {
        LIST-STYLE-POSITION: inside;
        border-spacing: 1000px;
        padding: 1px;
        border: #527787;
    }

    sStyleRowType {
        FONT-WEIGHT: normal;
        FONT-SIZE: 8pt;
        LINE-HEIGHT: 19px;
        FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif;
    }

    .backroundcolor {
        background-color: #65C7BD;
    }
    

    /* For changing the chosen height...*/
    .chosen-container-single .chosen-single {
        height: 33px;
        line-height: 29px;
        width:100%;
    }
    .chosen-container-single .chosen-single div b {
        background: url('../Content/chosen-sprite.png') no-repeat scroll 0 6px rgba(0, 0, 0, 0);
    }
    .chosen-container-active.chosen-with-drop .chosen-single div b {
        background-position: -18px 6px;
    }


    .chosen-container-single .chosen-single abbr {
        background: url('../Content/chosen-sprite.png') no-repeat scroll -42px 3px rgba(0, 0, 0, 0);
    }

    .chosen-container-single .chosen-single abbr:hover {
        background-position: -42px 3px;
    }

    .chosen-container a {
        cursor: pointer;
    }

    /*Metro is overriding chosen.. so added this..*/
    .metro abbr {
        border-bottom: 1px dotted #999999;
        cursor: pointer;
        text-decoration: none;
    }

    div.DataTables_sort_wrapper {
        font-weight:bold;
    }
    .metro .table th, .metro .table td {
    font-size:9pt;
    word-break:break-all;
}
   
.chosen-container .chosen-results li {
    
    font-size:9pt;
}
.switch > div > span.switch-default {
    background: none repeat scroll 0 0 #f00;
    color: #000000;
}
.switch > div > span.switch-default {
    background: none repeat scroll 0 0  #ce4c4c;
    color: #000000;
}

.switch > div > span.switch-primary {
    background: none repeat scroll 0 0 #45ba07;
    color: #FFFFFF;
}
.Quickview {
    background-color: #DDDDDD !important;
    
    }
    .Quickview tr {
        background-color: #DDDDDD !important;
    }
</style>


